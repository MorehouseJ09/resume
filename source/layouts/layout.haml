!!! html	
%html
	%head
		%meta{:charset => "utf-8"}
		%meta{:content => "IE=edge,chrome=1", "http-equiv" => "X-UA-Compatible"}
		%title= data.page.title
		= stylesheet_link_tag "normalize", data.page.css
		= stylesheet_link_tag "normalize", data.page.css_print, :media => "print"
		= javascript_include_tag data.page.javascripts

	%body
		= yield





